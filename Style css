        <div id="js-box" class="code-wrap code-box box active" role="region" aria-label="JS">


          <pre tabindex="0"><code data-lang="javascript" id="actual-js-code" class=" cm-s-default"><span class="cm-comment">// Handle contact form submission</span>

<span class="cm-variable">document</span>.<span class="cm-property">getElementById</span>(<span class="cm-string">"contactForm"</span>).<span class="cm-property">addEventListener</span>(<span class="cm-string">"submit"</span>, <span class="cm-keyword">function</span>(<span class="cm-def">event</span>) {

  <span class="cm-variable-2">event</span>.<span class="cm-property">preventDefault</span>(); <span class="cm-comment">// Prevent page reload</span>

  <span class="cm-comment">// Get values</span>

  <span class="cm-keyword">let</span> <span class="cm-def">name</span> <span class="cm-operator">=</span> <span class="cm-variable">document</span>.<span class="cm-property">getElementById</span>(<span class="cm-string">"name"</span>).<span class="cm-property">value</span>;

  <span class="cm-keyword">let</span> <span class="cm-def">email</span> <span class="cm-operator">=</span> <span class="cm-variable">document</span>.<span class="cm-property">getElementById</span>(<span class="cm-string">"email"</span>).<span class="cm-property">value</span>;

  <span class="cm-keyword">let</span> <span class="cm-def">message</span> <span class="cm-operator">=</span> <span class="cm-variable">document</span>.<span class="cm-property">getElementById</span>(<span class="cm-string">"message"</span>).<span class="cm-property">value</span>;

  <span class="cm-comment">// Show success message</span>

  <span class="cm-variable">document</span>.<span class="cm-property">getElementById</span>(<span class="cm-string">"formMessage"</span>).<span class="cm-property">innerText</span> <span class="cm-operator">=</span> 

    <span class="cm-string-2">`Thank you, ${</span><span class="cm-variable-2">name</span><span class="cm-string-2">}</span><span class="cm-string-2">! Your message has been sent.`</span>;

  <span class="cm-comment">// Reset form</span>

  <span class="cm-variable">document</span>.<span class="cm-property">getElementById</span>(<span class="cm-string">"contactForm"</span>).<span class="cm-property">reset</span>();

});</code></pre>

