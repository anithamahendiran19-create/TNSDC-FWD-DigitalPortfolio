          <pre tabindex="0"><code data-lang="css" id="actual-css-code" class=" cm-s-default"><span class="cm-comment">/* General Styles */</span>

<span class="cm-tag">body</span> {

  <span class="cm-property">margin</span>: <span class="cm-number">0</span>;

  <span class="cm-property">font-family</span>: <span class="cm-variable">Arial</span>, <span class="cm-atom">sans-serif</span>;

  <span class="cm-property">line-height</span>: <span class="cm-number">1.6</span>;

  <span class="cm-property">background</span>: <span class="cm-atom">#f4f4f9</span>;

  <span class="cm-property">color</span>: <span class="cm-atom">#333</span>;

  <span class="cm-property">text-align</span>: <span class="cm-atom">center</span>;

}

<span class="cm-comment">/* Header */</span>

<span class="cm-tag">header</span> {

  <span class="cm-property">background</span>: <span class="cm-atom">#333</span>;

  <span class="cm-property">color</span>: <span class="cm-atom">#fff</span>;

  <span class="cm-property">padding</span>: <span class="cm-number">30px</span> <span class="cm-number">20px</span>;

}

<span class="cm-tag">header</span> <span class="cm-tag">img</span> {

  <span class="cm-property">width</span>: <span class="cm-number">150px</span>;

  <span class="cm-property">height</span>: <span class="cm-number">150px</span>;

  <span class="cm-property">border-radius</span>: <span class="cm-number">50%</span>;

  <span class="cm-property">border</span>: <span class="cm-number">4px</span> <span class="cm-atom">solid</span> <span class="cm-atom">#fff</span>;

  <span class="cm-property">margin-bottom</span>: <span class="cm-number">10px</span>;

}

