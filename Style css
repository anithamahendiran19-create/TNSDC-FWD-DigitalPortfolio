  <span class="cm-property">display</span>: <span class="cm-atom">inline-block</span>;

  <span class="cm-property">background</span>: <span class="cm-atom">#555</span>;

  <span class="cm-property">color</span>: <span class="cm-keyword">white</span>;

  <span class="cm-property">padding</span>: <span class="cm-number">8px</span> <span class="cm-number">15px</span>;

  <span class="cm-property">margin</span>: <span class="cm-number">5px</span>;

  <span class="cm-property">border-radius</span>: <span class="cm-number">20px</span>;

}

<span class="cm-comment">/* Projects */</span>

<span class="cm-qualifier">.project-grid</span> {

  <span class="cm-property">display</span>: <span class="cm-atom">grid</span>;

  <span class="cm-property">grid-template-columns</span>: <span class="cm-variable cm-callee">repeat</span>(<span class="cm-variable">auto-fit</span>, <span class="cm-variable cm-callee">minmax</span>(<span class="cm-number">250px</span>, <span class="cm-number">1fr</span>));

  <span class="cm-property">gap</span>: <span class="cm-number">20px</span>;

}

