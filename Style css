  <span class="cm-property">padding</span>: <span class="cm-number">40px</span> <span class="cm-number">20px</span>;

  <span class="cm-property">max-width</span>: <span class="cm-number">900px</span>;

  <span class="cm-property">margin</span>: <span class="cm-atom">auto</span>;

}

